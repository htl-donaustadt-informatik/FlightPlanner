<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
  <system.data>
    <DbProviderFactories >
      <!-- type(class) = MySql.Data.MySqlClient.MySqlClientFactory, assembly = MySql.Data -->
      <!-- Version=6.7.4.0, Culture=neutral, PublicKeyToken=c5687fc88969c44d -->
      <add name="MySQL Data Provider" invariant="MySql.Data.MySqlClient" description=".Net Framework Data Provider for MySQL" type="MySql.Data.MySqlClient.MySqlClientFactory, MySql.Data" />
    </DbProviderFactories>
  </system.data>

  <connectionStrings>
    <add name="FlightPlannerConnection" providerName="System.Data.SqlClient" connectionString="Data Source=(LocalDB)\MSSQLLocalDb;Initial Catalog=FlightPlanner;Integrated Security=SSPI;" />
    <!--<add name="FlightPlannerConnection" providerName="MySql.Data.MySqlClient" connectionString="Data Source = if-webserver.htl-donaustadt.at; Port = 3306; Initial Catalog = FlightPlanner; Uid = FlightPlannerUser; Pwd = 12341234;" />-->
  </connectionStrings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="System.Buffers" publicKeyToken="cc7b13ffcd2ddd51" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.0.3.0" newVersion="4.0.3.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>